{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "NODE_TLS_REJECT_UNAUTHORIZED": "0",
    "MONGODB_TLS_ALLOW_INVALID_CERTIFICATES": "true",
    "NODE_OPTIONS": "--tls-min-v1.2"
  }
}